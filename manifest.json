{
  "name": "Rozetka",
  "version": "1.1",
  "description": "sort items in Rozetka without refresh",
  "permissions": ["tabs", "*://*.rozetka.com.ua/*"],
  "content_scripts": [
    {
      "matches": ["*://*.rozetka.com.ua/*"],
      "js": ["button.min.js", "index.user.min.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}